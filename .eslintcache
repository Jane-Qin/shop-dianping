mock/products/${path}.json?rowIndex=${rowIndex}&pageSize=${pageSize}`\r\n    }\r\n}","D:\\project\\react-project\\my-dianping-app\\src\\redux\\modules\\entities\\products.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\redux\\modules\\entities\\comments.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\redux\\modules\\entities\\orders.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\redux\\modules\\entities\\shops.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\containers\\Home\\index.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\containers\\Home\\components\\Headline\\index.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\containers\\Home\\components\\Discount\\index.js",["120"],"D:\\project\\react-project\\my-dianping-app\\src\\containers\\Home\\components\\LikeList\\index.js",["121"],"import React, { Component } from 'react';\r\nimport LikeItem from \"../LikeItem\";\r\nimport Loading from '../../../../components/Loading';\r\nimport \"./style.css\";\r\n\r\nclass LikeList extends Component {\r\n    constructor(props){\r\n        super(props);\r\n        this.myRef = React.createRef();\r\n        this.removeListener=false;   //是否移除事件监听标志位\r\n        this.handleScroll=this.handleScroll.bind(this)\r\n    }\r\n    render() {\r\n        const {data,pageCount}=this.props;\r\n        return (\r\n            <div ref={this.myRef} className=\"likelist\">\r\n                <div className=\"likeList__header\">猜你喜欢</div>\r\n                <div className=\"likeList__list\">\r\n                    {\r\n                        data.map((item,index)=>{\r\n                            return <LikeItem key={index} data={item}/>\r\n                        })\r\n                    }\r\n                </div>\r\n                {\r\n                    pageCount<3?(\r\n                        <Loading/>\r\n                    ):(\r\n                        <a className=\"likeList__viewAll\">查看更多</a>\r\n                    )\r\n                }\r\n            </div>\r\n        );\r\n    }\r\n    componentDidMount(){\r\n        //利用redux缓存层作用\r\n        if(this.props.pageCount<3){\r\n            document.addEventListener(\"scroll\",this.handleScroll);\r\n        }else{\r\n            this.removeListener=true;\r\n        }\r\n        \r\n        if(this.props.pageCount===0){\r\n            this.props.fetchData();\r\n        }\r\n    }\r\n    componentWillUnmount(){\r\n        //解除事件监听，防止内存泄漏\r\n        if(!this.removeListener){\r\n            document.removeEventListener(\"scroll\",this.handleScroll);\r\n        }\r\n\r\n    }\r\n    componentDidUpdate(){\r\n        if(this.props.pageCount>=3&& !this.removeListener){\r\n            //滚动加载3次后，移除事件监听\r\n            document.removeEventListener(\"scroll\",this.handleScroll);\r\n            this.removeListener=true;\r\n        }\r\n    }\r\n    //处理屏幕滚动事件，实现加载更多的效果\r\n    handleScroll(){\r\n        const scrollTop=document.documentElement.scrollTop||document.body.scrollTop; //页面滚动的距离(兼容不同浏览器)获取当前页面滚动条纵坐标的位置：\r\n        const screenHeight=document.documentElement.clientHeight;   //页面可视范围高度\r\n        const likeListTop=this.myRef.current.offsetTop;     //likelist距离顶部的距离\r\n        const likeListHeight=this.myRef.current.offsetHeight;   //lkielist内容的高度\r\n        //滚动距离大于可滚动高度时，加载数据\r\n        if(scrollTop>=(likeListTop+likeListHeight-screenHeight)){\r\n            this.props.fetchData();\r\n        }\r\n    }\r\n}\r\n\r\nexport default LikeList;\r\n ","D:\\project\\react-project\\my-dianping-app\\src\\containers\\Home\\components\\LikeItem\\index.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\components\\Loading\\index.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\containers\\Home\\components\\HomeHeader\\index.js",["122"],"import React, { Component } from 'react';\r\nimport {Link} from \"react-router-dom\";\r\nimport \"./style.css\";\r\n\r\nclass HomeHeader extends Component {\r\n    render() {\r\n        return (\r\n    