tReducer,applyMiddleware(thunk));\r\n}\r\nexport default store;\r\n","D:\\project\\react-project\\my-dianping-app\\src\\containers\\App\\index.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\redux\\middleware\\api.js",["134"],"import { get } from \"../../utils/request\"; \r\n//代表使用redux-thunk进行一次网络请求的过程\r\n// {\r\n//     FETCH_DATA:{\r\n//         types:['request','success','fail'],\r\n//         endpoint:url,\r\n//         schema: {   //代表领域实体的结构  定义在每个领域实体中  转成扁平化数据结构\r\n//             id:\"product_id\"  //那个属性代表id值\r\n//             name: 。。。           //让中间件知道当前处理的是哪一个领域实体\r\n//         }\r\n//     },\r\n//     params\r\n// }\r\n\r\n//定义中间件处理上面的action,将redux-thunk处理网络请求的模板方法的调用进行进一步的简化和封装\r\n//经过中间件处理的action所具有的标识\r\nexport const FETCH_DATA = \"FETCH DATA\";\r\n\r\nexport default store => (next) => (action) => {\r\n  const callAPI = action[FETCH_DATA];\r\n  if (typeof callAPI === \"undefined\") {\r\n    return next(action);\r\n  }\r\n  const { endpoint, schema, types } = callAPI;\r\n  if (typeof endpoint !== \"string\") {\r\n    throw new Error(\"endPoint必须为字符串类型的URL\");\r\n  }\r\n  if (!schema) {\r\n    throw new Error(\"必须指定领域实体的schema\");\r\n  }\r\n  if (!Array.isArray(types) && types.length !== 3) {\r\n    throw new Error(\"需要指定一个包含了3个action type的数组\");\r\n  }\r\n  if (!types.every((type) => typeof type === \"string\")) {\r\n    throw new Error(\"action type必须为字符串类型\");\r\n  }\r\n  const actionWith=data=>{    //增强action，保证action原有param传递下去\r\n      const finalAction={...action,...data};\r\n      delete finalAction[FETCH_DATA];\r\n      return finalAction\r\n  }\r\n  const [requestType, successType, failureType] = types;\r\n\r\n  next( actionWith({ type: requestType }) );\r\n  return fetchData(endpoint, schema).then(\r\n    (response) =>\r\n      next(actionWith({\r\n        type: successType,\r\n        response,\r\n      })),\r\n    (error) =>\r\n      next(actionWith({\r\n        type: failureType,\r\n        error: error.message || \"获取数据失败\",\r\n      }))\r\n  );\r\n};\r\n//执行网络请求\r\nconst fetchData = (endpoint, schema) => {\r\n  return get(endpoint).then((data) => { \r\n    return normalizeData(data, schema);\r\n  });\r\n};\r\n//根据schema，将获取的数据扁平化处理\r\nconst normalizeData = (data, schema) => {\r\n  const { id, name } = schema;\r\n  let keyValueObj = {\r\n    //存储扁平化数据\r\n  };\r\n  let ids = []; //存储获取到的id\r\n  if (Array.isArray(data)) {\r\n    data.forEach((item) => {\r\n      keyValueObj[item[id]] = item;\r\n      ids.push(item[id]);\r\n    });\r\n  } else {\r\n    keyValueObj[data[id]] = data;\r\n    ids.push(data[id]);\r\n  }\r\n  return {\r\n    [name]: keyValueObj,\r\n    ids,\r\n  };\r\n};\r\n","D:\\project\\react-project\\my-dianping-app\\src\\redux\\modules\\app.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\redux\\modules\\index.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\utils\\request.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\containers\\Home\\index.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\containers\\ProductDetail\\index.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\redux\\modules\\detail.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\components\\ErrorToast\\index.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\redux\\modules\\home.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\redux\\modules\\entities\\index.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\redux\\modules\\entities\\products.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\utils\\url.js",["135"],"export default {\r\n    getProductList:(path,rowIndex,pageSize)=>{\r\n        return `/mock/products/${path}.json?rowIndex=${rowIndex}&pageSize=${pageSize}`\r\n    }\r\n}","D:\\project\\react-project\\my-dianping-app\\src\\components\\Footer\\index.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\redux\\modules\\entities\\comments.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\redux\\modules\\entities\\shops.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\containers\\Home\\components\\Headline\\index.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\redux\\modules\\entities\\orders.js",[],"D:\\project\\react-project\\my-dianping-app\\src\\containers\\Home\\components\\Discount\\index.js",["136"],"import React, { Component } from \"react\";\r\nimport {Link} from \"react-router-dom\"\r\nimport \"./style.css\";\r\n\r\nclass Discount extends Component {\r\n  render() {\r\n    const data = this.props.data;\r\n    return (\r\n      <div className=\"discount\">\r\n        <a className=\"discount__header\" >\r\n          <span className=\"discount__title\">超值特惠</span>\r\n          <span className=\"discount__more\">更多优惠</span>\r\n          <span className=\"discount__arrow\"></span>\r\n        </a>\r\n        <div className=\"discount__content\">\r\n          {data.map((item, index) => {\r\n            return (\r\n              <Link to={`/detail/${item.id}`} className=\"discount__item\" key={item.id}>\r\n                <div className=\"discount__itemPic\">\r\n                  <img width=\"100%\" height=\"100%\" src={item.picture}  alt=\"\"/>\r\n                </div>\r\n                <div className=\"discount__itemTitle\">{item.shop}</div>\r\n                <div className=\"discount__itemPriceWrapper\">\r\n                  <ins className=\"discount__itemCurrentPrice\">\r\n                    {item.currentPrice}\r\n                  </ins>\r\n                  <del className=\"dicsount__itemOldPrice\">{item.oldPrice}</del>\r\n                </div>\r\n              </Link>\r\n            );\r\n          })}\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default Discount;","D:\\project\\react-project\\my-dianping-app\\src\\containers\\Home\\components\\HomeHeader\\index.js",["137"],"import React, { Component } from 'react';\r\nimport {Link} from \"react-router-dom\";\r\nimport \"./style.css\";\r\n\r\nclass HomeHeader ex